<p id="notice"><%= notice %></p>

<h1>Todo Item</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todo_item25s.each do |todo_item25| %>
      <tr>
        <td><%= todo_item25.description %></td>
        <td><%= link_to 'Show', todo_item25 %></td>
        <td><%= link_to 'Edit', edit_todo_item25_path(todo_item25) %></td>
        <td><%= link_to 'Destroy', todo_item25, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

